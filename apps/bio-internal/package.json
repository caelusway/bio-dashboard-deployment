{
  "name": "@bio-sync/bio-internal",
  "version": "0.1.0",
  "private": true,
  "description": "BioProtocol internal Twitter API service built with Bun and Elysia.",
  "type": "module",
  "scripts": {
    "dev": "bun run src/index.ts",
    "check": "tsc --noEmit",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "test": "bun test",
    "migrate:twitter": "bun run scripts/migrate-twitter.ts",
    "migrate:legacy-growth": "bun run scripts/migrate-legacy-growth.ts",
    "seed:growth-sources": "bun run scripts/seed-growth-sources.ts",
    "import:marketing-csv": "bun run scripts/import-marketing-csv.ts",
    "twitter:sync": "bun run src/jobs/twitterSync.ts",
    "twitter:backfill-followers": "bun run scripts/backfill-followers.ts",
    "growth:backfill": "bun run scripts/backfill-growth.ts",
    "twitter:backfill-follower-history": "bun run scripts/backfill-follower-history.ts",
    "drizzle:migrate": "bun run scripts/run-migrations.ts"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.4.0",
    "@elysiajs/static": "^1.4.6",
    "@elysiajs/swagger": "^1.0.0",
    "@supabase/supabase-js": "^2.39.7",
    "drizzle-orm": "^0.44.6",
    "elysia": "^1.0.0-beta.3",
    "postgres": "^3.4.4",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bun": "^1.3.0",
    "@types/node": "^22.5.0",
    "drizzle-kit": "^0.31.5",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^48.2.7",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4"
  }
}
